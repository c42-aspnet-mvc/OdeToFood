@model IEnumerable<OdeToFood.Models.RestaurantViewModel>

<div id="restaurantList">
    @foreach (var item in Model)
    {
        <div>
            <h4>@item.Name</h4>
            <div>@item.City, @item.Country</div>
        </div>
        <div>
            Reviews: @item.CountOfReviews
        </div>
    }
</div>